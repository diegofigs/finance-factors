{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["src", "test", "README.md"],
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "docdash": {
        "static": true,
        "sort": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
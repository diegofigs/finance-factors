    "calculations",
    "webpack2",
    "es6",
    "es6-modules",
    "umd",
    "umd-modules"
  ],
  "author": "Diego Figueroa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegofigs/finance-factors/issues"
  },
  "homepage": "https://github.com/diegofigs/finance-factors#readme",
  "standard": {
    "ignore": [
      "lib"
    ]
  },
  "dependencies": {
    "rimraf": "^2.6.1"
  }
}

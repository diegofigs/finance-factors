{
  "name": "finance-factors",
  "version": "0.1.4",
  "description": "Small library for calculation of basic financial factors.",
  "main": "./lib/finance-factors.min.js",
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "chai": "3.4.1",
    "mocha": "2.3.4",
    "webpack": "^2.0.0"
  },
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack",
    "clean": "rimraf lib",
    "dev": "NODE_ENV=staging webpack --progress --colors --watch",
    "test": "npm run build && mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegofigs/finance-factors.git"
  },
  "keywords": [
    "finance",
